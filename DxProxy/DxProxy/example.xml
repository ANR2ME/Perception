<?xml version="1.0"?>
<shaderConfig>
  <rules>
    <!-- constantName="" startReg="" -  At least one of name or reg is required, optionally both can be supplied -->
    <rule id="1" constantType="MatrixC" modToApply="3" startReg="4"/>
    <!-- partialName="false" is false if not specified -->
    <rule id="2" constantName ="Proj" partialName="true" constantType="MatrixC" modToApply="2" startReg="8"/>
  </rules>

  <!-- Default rules to apply to all shaders (optional) -->
  <defaultRuleIDs>
    <ruleID id="1" />
    <ruleID id="2" />
    <ruleID id="3" />
  </defaultRuleIDs>

  <!-- Shader specific rule collections are optional (if both default and shader specific are left out then no rules will ever be applied) -->
  <shaderSpecificRuleIDs shaderHash="12345">
    <ruleID id="3" />
  </shaderSpecificRuleIDs>
</shaderConfig>
